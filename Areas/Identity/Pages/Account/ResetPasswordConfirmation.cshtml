@page
@model ResetPasswordConfirmationModel
@{
    ViewData["Title"] = "Reset password confirmation";
}

<h1>@ViewData["Title"]</h1>
<p>
    Your password has been reset. Please <a asp-page="./Login" data-ajax="true" data-ajax-method="GET" data-ajax-mode="replace" data-ajax-update="#Display">click here to log in</a>.
</p>
